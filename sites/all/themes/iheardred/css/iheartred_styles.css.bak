/* BOXPLOTS SIND STYLISCH WIE KINYE WEST, JUNGE! */
.container {
  width: 100%;
  max-width: 600px;
  margin: calc(2rem + 1rem) 0 2% 0;
  padding: 0;
  float: left; }

.boxplot {
  width: 70%;
  height: 40px;
  max-width: 600px;
  position: relative;
  float: left; }

.box {
  width: 100%;
  height: calc(40px + 2rem);
  position: absolute; }

.linie {
  height: 20px;
  border-bottom: 1px solid #999; }

.whisker {
  width: 100%;
  height: 15px;
  margin-top: 12.5px;
  border-left: 1px solid #999;
  border-right: 1px solid #999; }

.thirdquant {
  background-color: #fff;
  background: repeating-linear-gradient(45deg, #999, #999 1px, transparent 1px, transparent 10px), linear-gradient(to bottom, #fff, #fff);
  width: 35%;
  height: 40px;
  border: 1px solid #999;
  border-right: 3px solid #27abf6; }

.median,
.mittel,
.interquart {
  width: 1px;
  border-right: 3px solid #ff5bbd;
  bottom: 0;
  z-index: 99; }

.interquart {
  border-right: 3px solid #29c95f;
  height: 40px; }

.mittel {
  top: 0; }

.median {
  border-left: 0px solid transparent;
  border-right: 3px solid #ff7d5f; }

.schild {
  display: inline-block;
  position: absolute;
  margin-top: -1rem;
  padding: 0 .5rem 0;
  font-weight: bold;
  color: #fff;
  background-color: #999;
  border-radius: 300px;
  text-align: center;
  z-index: 100; }

.schild.s_median,
.schild.s_average {
  margin-top: -3rem;
  background-color: #ff5bbd; }

.schild.s_average {
  margin-top: 4rem; }

.schild.s_median {
  background-color: #ff7d5f; }

.schild.s_first_quantil {
  background-color: #29c95f;
  margin-top: 2rem; }

.schild.s_third_quantil {
  background-color: #27abf6; }

/* Legende fuer Boxplots */
.legend {
  float: right;
  display: block;
  width: 20%;
  /* Legende vertikal an Boxplot ausrichten */
  margin-top: -2.3rem;
  margin-bottom: 1rem;
  line-height: 1;
  font-weight: bold; }
  .legend .item-list ul li {
    background: none, #fff;
    list-style-type: none;
    padding: 5px;
    margin: 5px; }
  .legend .item-list ul li:nth-child(1) {
    background: none, #29c95f;
    color: #fff; }
  .legend .item-list ul li:nth-child(2) {
    background: none, #ff5bbd;
    color: #fff; }
  .legend .item-list ul li:nth-child(3) {
    background: none, #ff7d5f;
    color: #fff; }
  .legend .item-list ul li:nth-child(4) {
    background: none, #27abf6;
    color: #fff; }

/* [Fix] Antwortmöglichkeiten Blöcke immer in neuer Zeile */
#delphi-question-change-question-form fieldset {
  clear: both; }

/* Breite des Textfeld für Titel der Frage an Seitenbreite anpassen */
#question-field, .item-title {
  width: 100%; }

/* Titel der Antwortmöglichkeiten beim Ausfüllen einer Umfrage hervorheben */
.answers > div > label {
  font-weight: bold; }

/* Fix damit Boxplots nichts mehr über Fieldset-Box hinausragen */
#survey-question-evaluation-form .fieldset-wrapper {
  margin-top: 55px; }

#survey-question-evaluation-form .title-long .fieldset-wrapper {
  margin-top: 80px; }

.boxplot-nodata {
  margin-top: -40px;
  margin-bottom: 10px; }

/* Abstand Antwortmöglichkeit Textfeld beim Beantworten der Umfrage */
.answers > .form-type-textfield {
  margin-top: 1rem; }

/* Radio-Buttons auf einer Höhe mit Labels */
.answers .form-type-radio label {
  margin-top: -4px; }

input.form-radio {
  vertical-align: sub; }

/* Radio-Buttons zur konfiguration der Fragen nebeneinander */
#delphi-question-change-question-form .form-type-radio,
#question-form .form-type-radio {
  width: auto;
  margin-right: 2rem; }

/* Boxplot Fieldset Formatierung */
#survey-question-evaluation-form fieldset legend {
  left: 0;
  height: auto;
  text-indent: 0;
  padding-left: 10px; }
  #survey-question-evaluation-form fieldset legend .fieldset-legend {
    font-weight: bold; }

/* Rahmen als zur Strukturierung auf Umfrageseite */
.answers > .form-type-textfield,
.answers > .form-type-radios {
  border-style: solid;
  border-width: 2px;
  padding: 0.5rem 1rem;
  margin-top: 0.5rem; }

.answers > .form-type-radios {
  padding-bottom: 4rem;
  margin-bottom: 0; }

/* Ein-/ausblendbare Kommentare als Buttons gestyled */
.toggleComments .form-type-checkbox {
  margin-top: -1rem;
  margin-bottom: 0; }
.toggleComments * {
  float: right; }
.toggleComments input {
  display: none; }
.toggleComments input[type=checkbox] + label {
  color: #777;
  background-color: #F2F2F2;
  cursor: pointer;
  padding: 4px 1em 4px 1em;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px; }
.toggleComments input[type=checkbox]:checked + label {
  background-color: #DBDBDB; }

#delphi-question-change-question-form *,
#question-form * {
  max-width: 800px; }

#delphi-question-change-question-form .answer-sets,
#question-form .answer-sets {
  max-width: 740px; }

.select-table .form-type-select {
  margin: auto; }

/* Hide elements on question group form */
.question-group-form .form-item-quantity,
.question-group-form #edit-back,
.question-group-form .form-type-radios {
  display: none !important; }

/* Highlight question group rows on question overview*/
.question-group-row td {
  background-color: lightyellow; }

.question-row td:first-child {
  padding-left: 20px; }

#edit-new-questiongroup {
  margin-right: 2em; }

/**
 * Style the survey progress bar.
 */
.progress-bar {
  background-color: #f4f4f4;
  border-radius: 4px;
  height: 20px;
  width: 100%;
  float: left;
  margin-top: 1em; }

.progress {
  background-color: lightblue;
  text-align: center;
  border-radius: inherit;
  min-width: 2em; }

/* Hide text on disabled textfields */
.question-content-wrapper input[type="text"]:disabled,
.evaluation-content-wrapper input[type="text"]:disabled {
  color: transparent; }

/* Style end survey form */
.form-item-feedback {
  margin-top: 2em;
  margin-bottom: 0; }

/* Always position the back button to the left */
.buttons-submit,
#survey-question-evaluation-form {
  float: left;
  clear: both; }
  .buttons-submit #edit-back,
  #survey-question-evaluation-form #edit-back {
    float: left; }
  .buttons-submit #edit-submit,
  #survey-question-evaluation-form #edit-submit {
    float: none; }

